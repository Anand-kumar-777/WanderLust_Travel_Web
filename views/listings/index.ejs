<% layout('layouts/boilerplate') -%>
<body>
    <h2>All Listings</h2>
    <!-- <form method="GET" action="/listings/new">
        <button>Create New List</button>
    </form> -->
    <ul>
        <% allListings.forEach(listing => { %>
            <li>
                <a href="/listings/<%= listing._id %>">
                    <%= listing.title %> - $<%= listing.price %>
                </a>
            </li>
        <% }) %>
    </ul>
</body>
